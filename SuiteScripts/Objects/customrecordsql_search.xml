    <instance scriptid="val_95876_6515607_sb1_637">
      <custrecordsql_description>Sample : Show Customer Transactions</custrecordsql_description>
      <custrecordsql_query>Select &#xd;
	C.altname as cust_name, &#xd;
	C.id as custid, &#xd;
	C.searchstage, &#xd;
	T.tranid as transaction_id, &#xd;
	T.id as trans_internal_id, &#xd;
	T.duedate,&#xd;
	BUILTIN.DF(T.status) as Status&#xd;
&#xd;
from customer C&#xd;
	JOIN Transaction T on (T.entity = C.id)&#xd;
&#xd;
where C.id = 4039</custrecordsql_query>
      <isinactive>F</isinactive>
      <name>Customer Transactions</name>
    </instance>
